package  {	import flash.display.MovieClip;		public class CarObstacle extends MovieClip {				public var lane;		public var relativeSpeed;		public function CarObstacle() {			// constructor code			this.gotoAndStop(Math.floor(1 + Math.random() * 6));						if (Math.random() < 0.5) {				this.lane = "top";								this.scaleX = -1.0;				this.relativeSpeed = 5.0 + 0.75 + 0.5 * Math.random();							} else {				this.lane = "bottom";				this.relativeSpeed = 0.75 + 0.5 * Math.random();			}					}	}	}